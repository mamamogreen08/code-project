import javax.swing.JOptionPane;

public class num3 {
    public static void main(String[] args) {
        String triangle = "";
        for (int i = 1; i <= 5; i++) {
            for (int j = 1; j <= i; j++) {
                triangle += "*";
            }
            triangle += "\n";
        }
        JOptionPane.showMessageDialog(null, triangle);
    }
}
