import javax.swing.JOptionPane;

public class num2 {
    public static void main(String[] args) {
        int[] arr = new int[10];
        for (int i = 0; i < arr.length; i++) {
            arr[i] = Integer.parseInt(JOptionPane.showInputDialog("Enter integer " + (i+1) + ":"));
        }
        int count7 = 0;
        for (int x : arr) {
            if (x == 7) count7++;
        }
        JOptionPane.showMessageDialog(null, "Number 7 appears " + count7 + " times.");
    }
}
