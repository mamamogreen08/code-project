import javax.swing.JOptionPane;

public class ask user repeatedly for num 1-100 {
    public static void main(String[] args) {
        int num;
        do {
            num = Integer.parseInt(JOptionPane.showInputDialog("Enter a number (1-100):"));
        } while (num < 1 || num > 100);
        JOptionPane.showMessageDialog(null, "Valid number: " + num);
    }
}
