import javax.swing.JOptionPane;

public class num12 {
    public static void main(String[] args) {
        int[] arr = new int[10];
        for (int i = 0; i < arr.length; i++) {
            arr[i] = Integer.parseInt(JOptionPane.showInputDialog("Enter integer " + (i+1) + ":"));
        }

        int valCheck = Integer.parseInt(JOptionPane.showInputDialog("Enter value to check:"));
        int count = 0;

        for (int x : arr) {
            if (x == valCheck) count++;
        }

        JOptionPane.showMessageDialog(null, valCheck + " appears " + count + " times. " +
                (count >= 3 ? "At least 3 times!" : "Less than 3 times."));
    }
}
